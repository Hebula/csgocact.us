<div id="cf-left">
	<div id="cf-header" class="content-div">
		<a id="create-game" class="noselect" rel="modal:open" href="#cf-modal-create-game">
			<span>Create</span>
			<div class="icon">
				<i class="fa fa-plus-square-o"></i>
			</div>
		</a>
		<table id="cf-labels">
			<tr id="cf-table-header">
				<th>Online</th>
				<th>Open Games</th>
				<th>Total Wagered</th>
			</tr>
			<tr id="cf-table-content">
				<td><span id="cf-online">0</span></td>
				<td><span id="cf-open-games">0</span></td>
				<td><span id="cf-wagered">0.00</span></td>
			</tr>
		</table>
		<div id="cf-sort-button" class="noselect asc">
			<i class="fa fa-sort-amount-asc" aria-hidden="true"></i>
		</div>
		<div id="cf-sort-button" class="noselect desc">
			<i class="fa fa-sort-amount-desc" aria-hidden="true"></i>
		</div>
	</div>
	<div id="cf-games" class="content-div">
		<div class="modal-spinner">
			<i class="fa fa-spinner fa-pulse fa-3x fa-fw"></i>
		</div>
		<div id="cf-games-header">
			<h5><span>Active Games</span></h5>
			<i class="fa fa-refresh noselect" id="cf-games-reload" aria-hidden="true"></i>
		</div>
		<div id="cf-table-wrapper">
			<div class="cf-games-table"></div>
		</div>
	</div>
</div>
<div id="cf-right">
	<div id="cf-advert-box" class="content-div">
		<h5>Want your advertisement displayed here? Send a message to <a href="/support">support</a></h5>
	</div>
	<div id="cf-promo-code" class="content-div">
		<h5>PROMO CODE</h5>
		<input id="cf-promo-input" type="text" placeholder="Enter code...">
		<button id="cf-promo-submit" class="" type="submit"><span>Enter</span><i class="fa fa-refresh fa-spin fa-3x fa-fw"></i></button>
	</div>
	<% if (typeof user == 'object' && user) { %>
		<div id="cf-user-history" class="content-div">
			<div class="header">
				<h5>
					<i class="fa fa-server" aria-hidden="true"></i> Your History <span class="game-amount" id="user-game-amount">0</span>
					<span id="cf-dropdown-trigger" class="opened noselect"><i class="fa fa-plus-square" aria-hidden="true"></i></span>
					<span id="cf-dropdown-trigger" class="closed noselect"><i class="fa fa-minus-square" aria-hidden="true"></i></span>
				</h5>
			</div>
			<div class="table-wrapper">
				<div class="cf-games-loader">
					<i class="fa fa-spinner fa-pulse fa-3x fa-fw"></i>
				</div>
				<table class="cf-table" id="cf-user-history-table"></table>
			</div>
		</div>
	<% } %>
	<div id="cf-history" class="content-div">
		<div class="header">
			<h5>
				<i class="fa fa-history" aria-hidden="true"></i> History <span class="game-amount" id="history-game-amount">0</span>
				<span id="cf-dropdown-trigger" class="opened noselect"><i class="fa fa-plus-square" aria-hidden="true"></i></span>
				<span id="cf-dropdown-trigger" class="closed noselect"><i class="fa fa-minus-square" aria-hidden="true"></i></span>
			</h5>
		</div>
		<div class="table-wrapper">
			<div class="cf-games-loader">
				<i class="fa fa-spinner fa-pulse fa-3x fa-fw"></i>
			</div>
			<table class="cf-table" id="cf-history-table"></table>
		</div>
	</div>
	<div id="cf-leaderboards" class="content-div">
		<div class="header">
			<h5>
				<i class="fa fa-signal" aria-hidden="true"></i> Leaderboards
				<span id="cf-dropdown-trigger" class="opened noselect"><i class="fa fa-plus-square" aria-hidden="true"></i></span>
				<span id="cf-dropdown-trigger" class="closed noselect"><i class="fa fa-minus-square" aria-hidden="true"></i></span>
			</h5>
		</div>
		<div class="table-wrapper">
			<div class="cf-games-loader">
				<i class="fa fa-spinner fa-pulse fa-3x fa-fw"></i>
			</div>
			<table class="cf-table" id="cf-leaderboards-table"></table>
		</div>
	</div>
</div>
